class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n=nums.size();
        unordered_map<int,int>m;
        for(int i=0;i<n;i++){ m[nums[i]]++;}
        int max=0;
        for(auto &p: m){
            if(p.second>(n/2)){max=(p.first);}
            }
            return max;
    }
};
