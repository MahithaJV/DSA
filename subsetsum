#include <stdio.h>

void printSubset(int subset[], int size) {
    printf("{ ");
    for (int i = 0; i < size; i++)
        printf("%d ", subset[i]);
    printf("}\n");
}

void subsetSumBacktrack(int arr[], int n, int index, int subset[], int subsetSize, int currentSum, int target) {
    // If current sum equals target, print the subset
    if (currentSum == target) {
        printSubset(subset, subsetSize);
        return;
    }

    // If we reach end of array or currentSum exceeds target, stop
    if (index == n || currentSum > target)
        return;

    // Include current element
    subset[subsetSize] = arr[index];
    subsetSumBacktrack(arr, n, index + 1, subset, subsetSize + 1, currentSum + arr[index], target);

    // Exclude current element (backtrack)
    subsetSumBacktrack(arr, n, index + 1, subset, subsetSize, currentSum, target);
}

int main() {
    int arr[] = {3, 4, 5};
    int n = 3;
    int target = 9;
    int subset[n]; // temporary array to store subset

    printf("Subsets with sum %d are:\n", target);
    subsetSumBacktrack(arr, n, 0, subset, 0, 0, target);

    return 0;
}
